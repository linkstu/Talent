@model Talent.service.ViewModels.ServiceVMs.ServiceListVM
@inject IStringLocalizer<Program> Localizer;

<wt:searchpanel vm="@Model" expanded="true" reset-btn="true">
<wt:row items-per-row="ItemsPerRowEnum.Four">
<wt:combobox field="Searcher.CategoryID" label-text="服务类别" items="Searcher.AllCategorys" empty-text="@Localizer["Sys.All"]" />
<wt:textbox field="Searcher.Name" />
<wt:combobox field="Searcher.IsEnabled" empty-text="@Localizer["Sys.All"]" />
</wt:row>
</wt:searchpanel>
<wt:grid vm="@Model" url="/service/Service/Search" hidden-checkbox="true"/>
