@model Talent.Base.ViewModels.IndustryCategoryVMs.IndustryCategoryVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.One">
<wt:textbox field="Entity.Category" />
<wt:switch field="Entity.IsEnabled" />
<wt:textbox field="Entity.SortIndex" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
