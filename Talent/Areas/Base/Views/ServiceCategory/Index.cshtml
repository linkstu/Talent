@model Talent.Base.ViewModels.ServiceCategoryVMs.ServiceCategoryListVM
@inject IStringLocalizer<Program> Localizer;

<wt:searchpanel vm="@Model" reset-btn="true">
<wt:row items-per-row="ItemsPerRowEnum.Four">
<wt:textbox field="Searcher.Name" />
<wt:combobox field="Searcher.Type" empty-text="@Localizer["Sys.All"]" />
<wt:combobox field="Searcher.IsEnabled" empty-text="@Localizer["Sys.All"]" />
</wt:row>
</wt:searchpanel>
<wt:grid vm="@Model" url="/Base/ServiceCategory/Search"/>
