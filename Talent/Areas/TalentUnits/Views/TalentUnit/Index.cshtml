@model Talent.TalentUnits.ViewModels.TalentUnitVMs.TalentUnitListVM
@inject IStringLocalizer<Program> Localizer;

<wt:searchpanel vm="@Model" reset-btn="true">
<wt:row items-per-row="ItemsPerRowEnum.Four">
<wt:textbox field="Searcher.Name" />
<wt:combobox field="Searcher.CategoryID" items="Searcher.AllCategorys" empty-text="@Localizer["Sys.All"]" />
<wt:textbox field="Searcher.Telephone" />
<wt:combobox field="Searcher.Status" empty-text="@Localizer["Sys.All"]" />
</wt:row>
</wt:searchpanel>
<wt:grid vm="@Model" url="/TalentUnits/TalentUnit/Search"/>
